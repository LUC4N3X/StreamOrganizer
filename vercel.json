{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/(.*)\\.(js|mjs|css|map|json|xml|txt|png|jpg|jpeg|gif|svg|ico|webp)$",
      "dest": "/public/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/public/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
